    <folder name="Actions">
        <folder name="Project">
            <file name="org-language-perl-action-TestPerlAction.instance">
                <attr name="instanceClass" stringvalue="org-netbeans-modules-project-ui-RunSingle"/>
                <attr name="instanceOf" stringvalue="javax.swing.Action"/>
                <attr name="instanceCreate" methodvalue="org.netbeans.modules.project.ui.actions.Actions.runSingle"/>
                <attr name="action" stringvalue="runSingle"/>
                <attr name="name" stringvalue="Run Back to Cursor"/>
                <attr name="iconBase" stringvalue="org/language/perl/action/file-parse-icon.png"/>
            </file>
        </folder>
    </folder>
    <folder name="Editors">
        <folder name="text">
            <folder name="x-perl">
                <folder name="CompletionProviders">
                    <file name="org-language-perl-completion-PerlCompletionProvider.instance"/>
                </folder>
                <folder name="FoldManager">
                    <file name="org-netbeans-editor-CustomFoldManager$Factory.instance"/>
                </folder>    
                <attr name="instanceCreate" methodvalue="org.language.perl.lexer.PerlTokenId.getLanguage"/>
                <attr name="instanceOf" stringvalue="org.netbeans.api.lexer.Language"/>
                <file name="org-language-perl-parser-PerlParserFactory.instance"/>
                <file name="org-language-perl-parser-SyntaxErrorsHighlightingTaskFactory.instance"/>
                <attr name="SystemFileSystem.localizingBundle" stringvalue="org.language.perl.Bundle"/>
                <folder name="FontsColors">
                    <folder name="NetBeans">
                        <folder name="Defaults">
                            <file name="org-language-perl-FontAndColors.xml" url="FontAndColors.xml">
                                <attr name="SystemFileSystem.localizingBundle" stringvalue="org.language.perl.Bundle"/>
                            </file>
                        </folder>
                    </folder>
                </folder>
            </folder>
        </folder>
